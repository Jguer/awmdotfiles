# ~/.gitconfig
[branch]
  autosetuprebase = always
[credential]
  helper = /usr/lib/git-core/git-credential-gnome-keyring
[commit]
	gpgsign = true
[push]
	default = current
[rebase]
  autostash = true
  autosquash = true
[include]
  path=~/.gitconfig-user
[status]
	submoduleSummary = true
[core]
	excludesfile = ~/.gitignore
[pull]
	rebase = true
